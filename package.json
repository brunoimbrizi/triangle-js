{
  "name": "triangle-wasm",
  "version": "1.0.0",
  "description": "Javascript wrapper around Triangle - A Two-Dimensional Quality Mesh Generator and Delaunay Triangulator",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Bruno Imbrizi",
    "url": "https://github.com/brunoimbrizi"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "array-unflat": "^1.0.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "load-svg": "^1.0.0",
    "normalize-path-scale": "^2.0.0",
    "opentype.js": "^1.3.3",
    "poly-parse": "^1.0.7",
    "svg-to-poly": "^1.0.0",
    "tinyify": "^3.0.0",
    "tweakpane": "^1.5.6"
  },
  "scripts": {
    "demo:minimal": "budo demo/minimal/:bundle.js --live --dir demo/minimal/",
    "demo:switches": "budo demo/switches/:bundle.js --live --dir demo/switches/",
    "demo:voronoi": "budo demo/voronoi/:bundle.js --live --dir demo/voronoi/",
    "build:minimal": "browserify demo/minimal/index.js -t babelify -p tinyify -o demo/minimal/bundle.js",
    "build:switches": "browserify demo/switches/index.js -t babelify -p tinyify -o demo/switches/bundle.js",
    "build:voronoi": "browserify demo/voronoi/index.js -t babelify -p tinyify -o demo/voronoi/bundle.js"
  },
  "keywords": [
    "conforming",
    "constrained",
    "delaunay",
    "triangulation",
    "quality mesh",
    "triangle",
    "pslg"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/brunoimbrizi/triangle-wasm.git"
  },
  "homepage": "https://github.com/brunoimbrizi/triangle-wasm",
  "bugs": {
    "url": "https://github.com/brunoimbrizi/triangle-wasm/issues"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
